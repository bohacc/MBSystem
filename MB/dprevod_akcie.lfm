      'WHERE'
      '  (ID=:ID or :ID IS NULL)'
      '   AND (UPPER(PRIJMENI) LIKE UPPER(:PRIJMENI) or :PRIJMENI IS NULL)'
      '   AND (UPPER(JMENO) LIKE UPPER(:JMENO) or :JMENO IS NULL)'
